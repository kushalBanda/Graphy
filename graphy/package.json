{
  "name": "graphy",
  "displayName": "Graphy",
  "description": "Create a markdown file which is a graphical representation of the Codebase",
  "publisher": "kushalBanda",
  "repository": {
    "type": "git",
    "url": "https://github.com/kushalBanda/Graphy.git"
  },
  "version": "0.0.4",
  "icon": "assets/logo/Graphy1.1.png",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:graphy.generateGraphy",
    "onCommand:linelens.refresh",
    "workspaceContains:**/package.json",
    "workspaceContains:**/*.py",
    "workspaceContains:**/*.js",
    "workspaceContains:**/*.ts"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "graphy.generateGraphy",
        "title": "Graphy: Generate Codebase Analysis"
      },
      {
        "command": "linelens.refresh",
        "title": "LineLens: Refresh Line Counts"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint client",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.3.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {}
}
